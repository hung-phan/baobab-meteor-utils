{
  "name": "baobab-meteor-utils",
  "version": "1.0.0",
  "description": "Baobab utils for Meteor",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "test": "mocha --compilers js:compiler.js 'test/**/*.spec.js'",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/baobab-meteor.min.js --config webpack.config.js",
    "build": "npm run build:lib && npm run build:umd",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hung-phan/baobab-meteor-utils.git"
  },
  "keywords": [
    "baobab",
    "meteor",
    "reactive",
    "tracker"
  ],
  "author": "Hung Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hung-phan/baobab-meteor-utils/issues"
  },
  "homepage": "https://github.com/hung-phan/baobab-meteor-utils#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-plugin-mocha": "^1.1.0",
    "lodash": "^3.10.1",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9"
  }
}
